{
  "name": "adonuxt",
  "version": "1.0.0",
  "author": "",
  "description": "",
  "license": "MIT",
  "private": true,
  "main": "server.js",
  "scripts": {
    "serve:dev": "npm run dev",
    "dev": "nodemon --watch app --watch bootstrap --watch config --watch start --watch .env -x node server.js",
    "build": "node ./ace nuxtbuild",
    "start": "cross-env NODE_ENV=production node server.js",
    "precommit": "npm run lint",
    "lint": "standard && eslint --ext .js,.vue resources/"
  },
  "dependencies": {
    "@adonisjs/ace": "5.0.8",
    "@adonisjs/auth": "3.0.7",
    "@adonisjs/bodyparser": "2.0.9",
    "@adonisjs/cors": "^1.0.2",
    "@adonisjs/fold": "^4.0.5",
    "@adonisjs/framework": "5.0.13",
    "@adonisjs/ignitor": "2.0.8",
    "@adonisjs/lucid": "6.1.3",
    "@adonisjs/session": "1.0.29",
    "@adonisjs/shield": "^1.0.4",
    "@nuxtjs/auth": "4.8.4",
    "cross-env": "6.0.3",
    "nuxt": "latest",
    "nuxt-buefy": "^0.3.17",
    "pg": "7.12.1",
    "youch": "^2.0.4"
  },
  "standard": {
    "global": [
      "use",
      "make"
    ]
  },
  "autoload": {
    "App": "./app"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.6.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "nodemon": "1.19.4",
    "standard": "14.3.1"
  }
}
